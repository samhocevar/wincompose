#: language/i18n/Text.resx:413 ID:DisabledToolTip
msgid "WinCompose is disabled"
msgstr "WinCompose on pois käytöstä"

#. The name of the "Insert" key on the keyboard
#: language/i18n/Text.resx:417 ID:KeyInsert
msgid "Insert"
